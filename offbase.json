{
  "name": "superblog",
  "description": "superblog",
  "image": "/typewriter.jpg",
  "require": {
    "theme": {
      "url": "https://github.com/fingertwiddler/theme.git"
    }
  },
  "routes": [{
    "from": "/assets/:path",
    "to": "/src/assets/{{path}}"
  }, {
    "from": "/:name.js",
    "to": "/app/engine/{{name}}.js",
    "type": "application/javascript"
  }, {
    "from": "/menu.png",
    "to": "/app/engine/menu.png"
  }, {
    "from": "/style.css",
    "to": "/app/engine/style.css"
  }, {
    "from": "/editor",
    "to": "/app/engine/editor.html"
  }, {
    "from": "/post",
    "to": "/app/engine/post.html"
  }, {
    "from": "/",
    "to": "/app/engine/index.html",
    "type": "text/html"
  }]
}
