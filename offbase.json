{
  "name": "superblog",
  "description": "superblog",
  "image": "/typewriter.jpg",
  "require": {
    "theme": {
      "url": "https://github.com/fingertwiddler/theme.git"
    }
  },
  "routes": [{
    "from": "/assets/:path",
    "to": "/home/src/assets/{{path}}"
  }, {
    "from": "/editor",
    "to": "/app/engine/editor.html"
  }, {
    "from": "/post",
    "to": "/app/engine/post.html"
  }, {
    "from": "/",
    "to": "/app/engine/index.html",
    "type": "text/html"
  }]
}
