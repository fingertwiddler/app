{
  "name": "superblog",
  "description": "superblog",
  "image": "/app/typewriter.jpg",
  "routes": [{
    "from": "/assets/:path",
    "to": "/home/src/assets/{{path}}"
  }, {
    "from": "/editor",
    "to": "/app/engine/editor.html"
  }, {
    "from": "/post",
    "to": "/app/engine/post.html"
  }, {
    "from": "/",
    "to": "/app/engine/index.html",
    "type": "text/html"
  }, {
    "from": "/module/:path",
    "to": "/{{path}}/index.js",
    "type": "application/javascript"
  }]
}
